<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC
	"-//SPRING//DTD BEAN//EN"
	"spring-beans-2.0.dtd">
<beans>
	<bean id="hello" class="com.sysdelphia.workq.test.HelloSpring">
		<constructor-arg>
			<value>hello</value>
		</constructor-arg>
		<property name="mood">
			<value>happy</value>
		</property>
	</bean>
	<bean id="proto" class="com.sysdelphia.workq.test.HelloSpring" scope="prototype">
		<constructor-arg>
			<value>proto</value>
		</constructor-arg>
		<property name="mood">
			<value>sad</value>
		</property>
	</bean>
</beans>
